<sdf version="1.6">
  <model name="dvrk">

  <include>
    <uri>model://Cart</uri>
    <name>SUJ</name>
    <pose>0 0 0 0 0 0</pose>>
  </include>

  <include>
    <uri>model://PSM</uri>
      <name>PSM1</name>
      <pose> -1.4637 -0.1016 0.92386 0 0 3.14159</pose>
  </include>

  <include>
    <uri>model://PSM</uri>
    <name>PSM2</name>
    <pose> 1.4637 -0.1016 0.92386 0 0 0</pose>
  </include>

  <include>
    <uri>model://PSM</uri>
    <name>PSM3</name>
    <pose> 0 1.5145 0.64556 0 0 1.5708</pose>
  </include>

  <include>
    <uri>model://ECM</uri>
    <name>ecm</name>
    <pose>0 0.85 0.9 0 0.735398 1.5708</pose>
  </include>


  <joint name='PSM1::rev_joint' type='revolute'>
   <child>PSM1::base_link</child>
   <parent>SUJ::SUJ_PSM1_L4</parent>
   <axis>
     <limit>
       <lower>-1.57</lower>
       <upper>1.57</upper>
     </limit>
     <dynamics>
       <damping>0</damping>
     </dynamics>
     <xyz>0 0 1</xyz>
   </axis>
 </joint>

 <joint name='PSM2::rev_joint' type='revolute'>
  <child>PSM2::base_link</child>
  <parent>SUJ::SUJ_PSM2_L4</parent>
  <axis>
    <limit>
      <lower>-1.57</lower>
      <upper>1.57</upper>
    </limit>
    <dynamics>
      <damping>0</damping>
    </dynamics>
    <xyz>0 0 1</xyz>
  </axis>
</joint>

<joint name='PSM3::rev_joint' type='revolute'>
 <child>PSM3::base_link</child>
 <parent>SUJ::SUJ_PSM3_L4</parent>
 <axis>
   <limit>
     <lower>-1.57</lower>
     <upper>1.57</upper>
   </limit>
   <dynamics>
     <damping>0</damping>
   </dynamics>
   <xyz>0 0 1</xyz>
 </axis>
</joint>

<joint name='ecm::fixed' type='fixed'>
  <child>ecm::base_link</child>
  <parent>SUJ::SUJ_ECM_L3</parent>
</joint>

  <plugin name='dvrk_plugins' filename='libdvrk_plugins.so'/>
  </model>
</sdf>
