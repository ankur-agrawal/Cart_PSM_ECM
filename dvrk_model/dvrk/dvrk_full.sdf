<sdf version="1.6">
  <model name="dvrk">

  <include>
    <uri>model://Cart</uri>
    <name>SUJ</name>
    <pose>0 0 0 0 0 0</pose>>
  </include>

  <include>
    <uri>model://PSM</uri>
      <name>PSM1</name>
      <pose>-1.9501 -0.6016 0.0848 0 0 -1.57</pose>
  </include>

  <include>
    <uri>model://PSM</uri>
    <name>PSM2</name>
    <pose> 1.9501 0.3984 0.0848 0 0 1.57</pose>

  </include>

  <include>
    <uri>model://PSM</uri>
    <name>PSM3</name>
    <pose> -0.5 2 -0.19 0 0 3.14159</pose>
  </include>

  <include>
    <uri>model://ECM</uri>
    <name>ecm</name>
    <!-- <pose>0.075 0.7932 0.92135 -1.5708 -0.735398 -1.5708</pose> -->
    <pose>0 0.85 0.9 0 0.735398 1.5708</pose>
    <!-- <pose>0 0 1.5 0 0 0</pose> -->
  </include>

  <!-- <joint name='SUJ::fixed' type='fixed'>
    <child>SUJ::base_link</child>
    <parent>world</parent>
  </joint> -->

  <joint name='PSM1::rev_joint' type='revolute'>
   <child>PSM1::remote_center_link</child>
   <parent>SUJ::SUJ_PSM1_L4</parent>
   <axis>
     <limit>
       <lower>-1.57</lower>
       <upper>1.57</upper>
     </limit>
     <dynamics>
       <damping>0</damping>
     </dynamics>
     <xyz>0 0 1</xyz>
   </axis>
 </joint>

 <joint name='PSM2::rev_joint' type='revolute'>
  <child>PSM2::remote_center_link</child>
  <parent>SUJ::SUJ_PSM2_L4</parent>
  <axis>
    <limit>
      <lower>-1.57</lower>
      <upper>1.57</upper>
    </limit>
    <dynamics>
      <damping>0</damping>
    </dynamics>
    <xyz>0 0 1</xyz>
  </axis>
</joint>

<joint name='PSM3::rev_joint' type='revolute'>
 <child>PSM3::remote_center_link</child>
 <parent>SUJ::SUJ_PSM3_L4</parent>
 <axis>
   <limit>
     <lower>-1.57</lower>
     <upper>1.57</upper>
   </limit>
   <dynamics>
     <damping>0</damping>
   </dynamics>
   <xyz>0 0 1</xyz>
 </axis>
</joint>

<joint name='ecm::fixed' type='fixed'>
  <child>ecm::base_link</child>
  <parent>SUJ::SUJ_ECM_L3</parent>
  <!-- <parent>world</parent> -->
</joint>

<!-- <plugin name='gazebo_ros_control' filename='libgazebo_ros_control.so'>
  <robotSimType>gazebo_ros_control/DefaultRobotHWSim</robotSimType>
  <robotNamespace>/dvrk/Cart</robotNamespace>
</plugin> -->
  <plugin name='dvrk_plugins' filename='libdvrk_plugins.so'/>
  </model>
</sdf>
