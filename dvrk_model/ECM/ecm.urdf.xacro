        </geometry>
      </collision>
    </link>
    <joint
      name="${prefix}_pitch_front_joint"
      type="continuous">
      <origin
        xyz="0 0 0.199000000000014"
        rpy="3.14 -1.5707963267949 0" />
      <parent
        link="${prefix}_yaw_link" />
      <child
        link="${prefix}_pitch_front_link" />
      <axis
        xyz="0 0 1" />
    </joint>
    <link
      name="${prefix}_pitch_bottom_link">
      <inertial>
        <origin
          xyz="0.14594 -0.026715 -0.00016488"
          rpy="0 0 0" />
        <mass
          value="0.35928" />
        <inertia
          ixx="0.00023901"
          ixy="0.00022767"
          ixz="-3.8706E-06"
          iyy="0.0030096"
          iyz="-7.279E-07"
