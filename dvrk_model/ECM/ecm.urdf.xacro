        <inertia
          ixx="0.00086598"
          ixy="7.8532E-10"
          ixz="2.5709E-09"
          iyy="0.0016561"
          iyz="-0.00015825"
          izz="0.00085001" />
      </inertial>
      <visual>
        <origin
          xyz="0 0 0"
          rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://dvrk_model/ECM//meshes/yaw_link.STL" />
        </geometry>
        <material
