        </material>
      </visual>
      <collision>
        <origin
          xyz="0 0 0"
          rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://dvrk_model/ECM//meshes/pitch_top_link.STL" />
        </geometry>
      </collision>
    </link>
    <joint
