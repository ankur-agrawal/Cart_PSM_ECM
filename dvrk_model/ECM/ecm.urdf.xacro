        </geometry>
        <material
          name="">
          <color
            rgba="0.64706 0.51765 0 1" />
        </material>
      </visual>
      <collision>
        <origin
          xyz="0 0 0"
          rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://dvrk_model/ECM//meshes/pitch_back_link.STL" />
