    <joint
      name="${prefix}_pitch_back_joint"
      type="continuous">
      <origin
        xyz="0 -0.00979999999999302 0.162430000000002"
        rpy="3.14 -1.5707963267949 0" />
      <parent
        link="${prefix}_yaw_link" />
      <child
        link="${prefix}_pitch_back_link" />
      <axis
        xyz="0 0 1" />
      <mimic joint="${prefix}_pitch_front_joint" multiplier="1"/>
    </joint>
    <link
