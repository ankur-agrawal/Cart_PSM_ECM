        rpy="0 0 0" />
      <parent
        link="${prefix}_pitch_front_link" />
      <child
        link="${prefix}_pitch_top_link" />
      <axis
        xyz="0 0 1" />
      <mimic joint="${prefix}_pitch_front_joint" multiplier="-1"/>
